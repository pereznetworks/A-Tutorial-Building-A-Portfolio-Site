extends layout.pug

block content
  nav.grid-container.portfolio-breadcrumb
    a(href='/') ‚Üê Back

  article.grid-container.portfolio-index

    .grid-x.grid-margin-x.small-up-2.medium-up-2.large-up-3

      each item in projects
        .cell
          a(href=`/projects/${item.id}`)
            img.thumbnail(src=`/img/${item.image_urls[0]}`)
            h5= item.project_name
